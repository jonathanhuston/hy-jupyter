{
  "name": "run-jupyter",
  "displayName": "Run in Jupyter",
  "description": "Send the current selection or line to Jupyter",
  "version": "0.0.1",
  "publisher": "jonathanhuston",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": ["Other"],
  "main": "./extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "eslint": "^4.6.1",
    "@types/node": "^8.0.26",
    "@types/mocha": "^2.2.42"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanhuston"
  },
  "activationEvents": ["onCommand:run-jupyter.fde"],
  "contributes": {
    "commands": [
      {
        "command": "run-jupyter.fde",
        "title": "Run in Jupyter"
      }
    ],
    "keybindings": [
      {
        "command": "run-jupyter.fde",
        "key": "cmd+shift+enter",
        "when": "editorTextFocus"
      }
    ]
  }
}
