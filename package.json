{
  "name": "hy-jupyter",
  "displayName": "Hy Jupyter",
  "description": "Evaluate a Hy form (S-expression) in Jupyter notebook",
  "version": "0.3.1",
  "publisher": "jonathanhuston",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Programming Languages", 
    "Other"
  ],
  "main": "./extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "eslint": "^4.6.1",
    "@types/node": "^8.0.26",
    "@types/mocha": "^2.2.42"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanhuston/vscode-hy-jupyter"
  },
  "activationEvents": [
    "onLanguage:hy"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hy-jupyter.evaluateForm",
        "title": "Hy Jupyter: Evaluate current form",
        "enablement": "editorLangId == hy"
      }
    ],
    "keybindings": [
      {
        "command": "hy-jupyter.evaluateForm",
        "key": "shift+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "hy-jupyter.evaluateForm",
        "key": "ctrl+enter",
        "when": "editorTextFocus"
      }
    ]
  }
}
